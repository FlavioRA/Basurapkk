<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/fondo1"
    tools:context=".contacto">

    <TextView
        android:id="@+id/textView11"
        android:layout_width="318dp"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_marginStart="56dp"
        android:layout_marginTop="88dp"
        android:layout_marginEnd="37dp"
        android:fontFamily="sans-serif-black"
        android:gravity="center"
        android:text="Quejas Y Sugerencias"
        android:textSize="20sp" />

    <ImageView
        android:id="@+id/imageView10"
        android:layout_width="59dp"
        android:layout_height="75dp"
        android:layout_alignParentTop="true"
        android:layout_marginTop="0dp"
        app:srcCompat="@drawable/agenda" />

    <ImageView
        android:id="@+id/imageView11"
        android:layout_width="59dp"
        android:layout_height="75dp"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_marginTop="6dp"
        android:layout_marginEnd="2dp"
        app:srcCompat="@drawable/logonegro" />

    <TextView
        android:id="@+id/textView12"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_marginTop="71dp"
        android:text="Contacto" />

    <EditText
        android:id="@+id/edtNombre"
        android:layout_width="262dp"
        android:layout_height="wrap_content"
        android:layout_alignStart="@+id/textView11"
        android:layout_alignEnd="@+id/textView11"
        android:layout_alignParentTop="true"
        android:layout_marginStart="28dp"
        android:layout_marginTop="127dp"
        android:layout_marginEnd="28dp"
        android:ems="10"
        android:hint="Nombre"
        android:inputType="textPersonName" />

    <EditText
        android:id="@+id/edtTelefono"
        android:layout_width="256dp"
        android:layout_height="wrap_content"
        android:layout_alignStart="@+id/edtNombre"
        android:layout_alignEnd="@+id/edtNombre"
        android:layout_alignParentTop="true"
        android:layout_marginStart="1dp"
        android:layout_marginTop="182dp"
        android:layout_marginEnd="5dp"
        android:ems="10"
        android:hint="Telefono"
        android:inputType="phone" />

    <EditText
        android:id="@+id/edtEmail"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignStart="@+id/edtNombre"
        android:layout_alignEnd="@+id/edtNombre"
        android:layout_alignParentTop="true"
        android:layout_marginStart="-1dp"
        android:layout_marginTop="240dp"
        android:layout_marginEnd="8dp"
        android:ems="10"
        android:hint="Correo Electronico"
        android:inputType="textEmailAddress" />

    <TextView
        android:id="@+id/textView18"
        android:layout_width="363dp"
        android:layout_height="105dp"
        android:layout_alignStart="@+id/edtEmail"
        android:layout_alignTop="@+id/imageView10"
        android:layout_alignEnd="@+id/edtEmail"
        android:layout_marginStart="-53dp"
        android:layout_marginTop="301dp"
        android:layout_marginEnd="-55dp"
        android:background="@color/colorPrimaryDark"
        android:hint="Escribe tu mensaje aqui..." />

    <TextView
        android:id="@+id/textView19"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignStart="@+id/textView18"
        android:layout_alignTop="@+id/imageView10"
        android:layout_alignEnd="@+id/textView18"
        android:layout_marginStart="17dp"
        android:layout_marginTop="422dp"
        android:layout_marginEnd="18dp"
        android:fontFamily="sans-serif-medium"
        android:gravity="center"
        android:text="Ojo! Si no escribes tus datos de contacto de forma correcta no podremos contactarte."
        android:textColor="@android:color/holo_red_dark" />

    <Button
        android:id="@+id/btnEnviar"
        style="@style/Widget.AppCompat.Button.Colored"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignStart="@+id/textView19"
        android:layout_alignEnd="@+id/textView19"
        android:layout_alignParentTop="true"
        android:layout_marginStart="76dp"
        android:layout_marginTop="492dp"
        android:layout_marginEnd="78dp"
        android:text="Enviar" />

</RelativeLayout>